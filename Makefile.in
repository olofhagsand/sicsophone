VPATH       	= @srcdir@
srcdir  	= @srcdir@
top_srcdir  	= @top_srcdir@
SUBDIRS     	= @SUBDIRS@ 

all:
	for subdir in $(SUBDIRS); do \
	   (cd $$subdir && $(MAKE) $@) || exit 1; \
	done

depend:
	for subdir in $(SUBDIRS); do \
	   (cd $$subdir && $(MAKE) $@) || exit 1; \
	done

clean:
	for subdir in $(SUBDIRS); do \
	   (cd $$subdir && $(MAKE) $@) || exit 1; \
	done

realclean:
	for subdir in $(SUBDIRS); do \
	   (cd $$subdir && $(MAKE) $@) || exit 1; \
	done
	rm -f Makefile config.log config.status
	rm -f include/config.h

install:
	for subdir in $(SUBDIRS); do \
	   (cd $$subdir && $(MAKE) $@) || exit 1; \
	done

tags:
	find $(top_srcdir) -name '*.[chyl]' -print | etags -

FORCE:
